Algoritmo IntercambiarValores
	Definir variable1,variable2,variableAuxiliar Como Entero
	Escribir 'Escriba dos variables'
	Leer variable1,variable2
	Escribir 'variable 1 inicial es: ',variable1
	Escribir 'variable 2 inicial es: ',variable2
	variableAuxiliar <- variable1
	variable1 <- variable2
	variable2 <- variableAuxiliar
	Escribir 'Variable 1 final es:',variable1
	Escribir 'Variable 2 final es:',variable2
FinAlgoritmo
